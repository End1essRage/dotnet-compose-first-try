name: dockercompose5352543807248780653
services:
  catalogservice:
    build:
      context: D:\Projects\Microservice Market Pet Projetc\RetailMarket
      dockerfile: CatalogService/Dockerfile
    depends_on:
      postgres_image:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DB_CONNECTION_STRING: host=postgres_image;port=5432;database=blogdb;username=bloguser;password=bloguser
    image: catalogservice
    networks:
      catalogservice-dev: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 80
      published: "27767"
      protocol: tcp
  postgres_image:
    environment:
      POSTGRES_DB: blogdb
      POSTGRES_PASSWORD: bloguser
      POSTGRES_USER: bloguser
    image: postgres:latest
    networks:
      catalogservice-dev: null
    ports:
    - mode: ingress
      target: 5432
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: db_volume
      target: /var/lib/postgresql/data
      volume: {}
networks:
  catalogservice-dev:
    name: dockercompose5352543807248780653_catalogservice-dev
    driver: bridge
volumes:
  db_volume:
    name: dockercompose5352543807248780653_db_volume