name: dockercompose5352543807248780653
services:
  catalogservice:
    build:
      context: D:\Projects\Microservice Market Pet Projetc\RetailMarket
      dockerfile: CatalogService/Dockerfile
    depends_on:
      postgres_image:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ConnectionStrings_DefaultConnection: '"User ID=postgresuser;Password=postgres;Server=postgres_image;Port=5432;Database=SampleCategory;"'
    image: catalogservice
    networks:
      catalogservice-dev: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 80
      published: "27767"
      protocol: tcp
  postgres_image:
    environment:
      POSTGRES_DB: SampleCategory
      POSTGRES_PASSWORD: postgres
      POSTGRES_USER: postgresuser
    image: postgres:latest
    networks:
      catalogservice-dev: null
    ports:
    - mode: ingress
      target: 5432
      published: "5433"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: db_volume
      target: /var/lib/postgresql/data
      volume: {}
networks:
  catalogservice-dev:
    name: dockercompose5352543807248780653_catalogservice-dev
    driver: bridge
volumes:
  db_volume:
    name: dockercompose5352543807248780653_db_volume